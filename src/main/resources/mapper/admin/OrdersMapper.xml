<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.net.mall.server.admin.mapper.OrdersMapper">

    <update id="finish">
        update `net-mall`.orders set status=3 where id=#{id}
    </update>


    <select id="page" resultType="com.net.mall.pojo.vo.OrdersVO">
        select * from `net-mall`.orders where status=2
    </select>


</mapper>